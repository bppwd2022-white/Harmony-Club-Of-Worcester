<p id="notice"><%= notice %></p>

<h1>Web Event Listings</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Location</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @web_event_listings.each do |web_event_listing| %>
      <tr>
        <td><%= web_event_listing.title %></td>
        <td><%= web_event_listing.location.title %></td>
        <td><%= link_to 'Show', web_event_listing %></td>
        <td><%= link_to 'Edit', edit_web_event_listing_path(web_event_listing) %></td>
        <td><%= link_to 'Destroy', web_event_listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Web Event Listing', new_web_event_listing_path %>
